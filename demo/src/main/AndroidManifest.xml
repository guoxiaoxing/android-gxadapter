<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.souche.android.fcadapter.demo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity android:name=".PullToRefreshActivity"/>
        <activity android:name=".AnimationActivity"/>
        <activity android:name=".HeaderAndFooterActivity"/>
        <activity android:name=".SectionActivity"/>
        <activity android:name=".MultipleItemActivity"/>
        <activity android:name=".EmptyViewActivity"/>
        <activity android:name=".DragAndSwipeActivity"/>
        <activity android:name=".RecyclerClickItemActivity"/>
        <activity android:name=".ExpandableActivity"/>
        <activity android:name=".StickyHeaderActivity"/>
    </application>


</manifest>